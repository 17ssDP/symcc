#!/bin/bash

runtime_dir="@SYM_RUNTIME_DIR@"
pass="@CMAKE_CURRENT_BINARY_DIR@/libSymbolize.so"

exec @CLANG_BINARY@                             \
     -Xclang -load -Xclang "$pass"              \
     "$@"                                       \
     -L"$runtime_dir"                           \
     -lSymRuntime                               \
     -Wl,-rpath,"$runtime_dir"                  \
     -Qunused-arguments
